Template.help.helpers
  title: ->
    RocketChat.settings.get 'Layout_Home_Title'
  body: ->
    RocketChat.settings.get 'Layout_Home_Body'
  userId: ->
    Meteor.userId()
  currentUser: ->
    x = undefined
    x = RocketChat.models.Users.findOne('_id': Meteor.userId())
    x
  networkCount: ->
    q = undefined
    x = undefined
    x = RocketChat.models.Users.find({}).fetch()
